
<!--pages/android/andview.wxml-->
<view class="page">
  <view class="page__hd">
    <view class="page__titles">{{title}}</view>
    <!--<view class="page__desc">文章</view>-->
  </view>
  <view class="page__bd">
    <view class="weui-article">
      <view class="weui-article__h1">{{secondtitle}}</view>
      <view class="weui-article__section">
        <view class="weui-article__title">项目设计</view>
        <view class="weui-article__section">
          <view class="weui-article__h3">1.1 开发平台</view>
          <view class="weui-article__p">{{tool}}
          </view>
          <!--<view class="weui-article__p">
            <image class="weui-article__imgs" src="{{imageUrlOne}}" mode="aspectFit"  />
          </view>-->
        </view>
        <view class="weui-article__section">
          <view class="weui-article__h3">1.2 技术细节</view>
          <view class="weui-article__p">
            {{skillDetail}}
          </view>
          <!--<view class="weui-article__p">
            <image class="weui-article__imgs" src="{{imageUrlOne}}" mode="aspectFit"  />
          </view>-->
        </view>

<view class="weui-article__section">
          <view class="weui-article__h3">1.3 具体功能</view>
          <view class="weui-article__p">
            {{detailFunction}}
          </view>
          <view class="weui-article__p">
            <image class="weui-article__imgs" src="{{imageUrlOne}}" mode="aspectFit"  />
          </view>
        </view>

        <view class="weui-article__section">
          <view class="weui-article__h3">1.4 项目分工</view>
          <view class="weui-article__p">
            {{detailWork}}
          </view>
          <!--<view class="weui-article__p">
            <image class="weui-article__imgs" src="{{imageUrlOne}}" mode="aspectFit"  />
          </view>-->
        </view>

        <view class="weui-article__section">
          <view class="weui-article__h3">1.5 下载地址</view>
          <view class="weui-article__p" bindtap="priviewImage">
            <image class="weui-article__imgs" src="{{downAddress}}" mode="aspectFit"  />
          </view>
         
          <!--<view class="weui-article__p">
            <image class="weui-article__imgs" src="{{imageUrlOne}}" mode="aspectFit"  />
          </view>-->
        </view>

         <view class="weui-footer">
              <view class="weui-footer__links">
                <navigator url="../about/aboutus" class="weui-footer__link" style="color: #16a5af;">关于E-X</navigator>
                <navigator url="../code/code" class="weui-footer__link" style="color: #16a5af;">联系我们</navigator>
              </view>
              <view class="weui-footer__text">Copyright © 2015-2017 dongzheng.dd@163.com</view>
            </view>
  

      </view>
    </view>
  </view>
</view>